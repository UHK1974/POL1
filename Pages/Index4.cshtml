            <div id="entry">

                <h2>Project</h2>
                <form id="form1" runat="server" method="post">
                    <table id="entryTable">
                        <tr>
                        <td>
                        <label for="txtDate">Date:</label>
                        </td>
                        <td>
                        <input type="date" id="txtDate"  asp-for="Date" />
                        </td>
                        </tr>

                        <tr>
                            <td>
                                <label for="textbox1">Project ID:</label>
                            </td>
                            <td>
                                <input type="text" id="textbox1" asp-for="ProjectID" />
                            </td>
                            <td>
                                <label for="textbox2">Project Name:</label>
                            </td>
                            <td>
                                <input type="text" id="textbox2" asp-for="ProjectName" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                <label for="textbox3">Start Date:</label>
                            </td>
                            <td>
                                <input type="date" id="textbox3" asp-for="StartDate" />
                            </td>
                            <td>
                                <label for="textbox4">End Date:</label>
                            </td>
                            <td>
                                <input type="date" id="textbox4" asp-for="EndDate" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                <label for="textbox5">Service Company:</label>
                            </td>
                            <td>
                                <input type="text" id="textbox5" asp-for="ServiceCompany" />
                            </td>
                            <td>
                                <label for="textbox5">Tean Lead Name:</label>
                            </td>
                            <td>
                                <input type="text" id="textbox5" asp-for="TeamLeadName" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <label for="textbox5">Total Team Member:</label>
                            </td>
                            <td>
                                <input type="text" id="textbox5" asp-for="TotalTeamMember" />
                            </td>
                            <td>
                                <label for="Status1">Status:</label>
                            </td>
                            <td style="padding: 10px;">
                                <select id="Status1" type="ddlStatus1" asp-for="Status">
                                    <option value="InProgress">In Progress</option>
                                    <option value="Completed">Completed</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <label for="irTextBox4">Project Scope:</label>
                            </td>
                            <td colspan="3">
                                <textarea id="irTextBox4" rows="3" cols="40" asp-for="ProjectScope"></textarea>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <button id="btnSubmit" type="submit">Submit</button>
                            </td>
                        </tr>
                    </table>
                </form>
            </div>
            <div id="dateTime">
                <span id="clock"></span>
