            var charCode = (evt.which) ? evt.which : evt.keyCode;

            // Allow numeric digits (0-9), backspace, arrow keys, and the period ('.') character
            if ((charCode >= 48 && charCode <= 57) || charCode === 8 || charCode === 37 || charCode === 39 || charCode === 46) {
                return true;
            }
        }




