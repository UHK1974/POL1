                                function InventoryTable(receiptNumber) {
                                    $.ajax({
                                        url: "/Index7?handler=GetInventoryTableData&receiptNumber=" + receiptNumber,
                                        type: "GET",
                                        success: function (data) {
                                            console.log(data);
                                            populateTableWithData(data);

                                        },
                                        error: function () {
                                            // Handle errors here
                                        }
                                    });
                                }


                                function populateTableWithData(data) {
                                    var table = $("#inventory-table");

                                    // Clear existing rows in the table, excluding the header row
                                    table.find("tr:gt(0)").remove();

                                    // Loop through the data and create a new row for each item
                                    data.forEach(function (item) {
                                        var newRow = $("<tr></tr>");

                                        newRow.append(`<td><input type="text" name="ItemName" value="${item.itemName}" /></td>`);
                                        newRow.append(`<td><input type="text" name="ItemCode" value="${item.itemCode}" /></td>`);
                                        newRow.append(`<td><input type="text" name="Description" value="${item.description}" /></td>`);
                                        newRow.append(`<td><input type="text" name="UOM" value="${item.uom}" /></td>`);
                                        newRow.append(`<td><input type="number"name="Quantity"  oninput="calculateTotal(1)" onkeypress="return isNumeric(evt);" size="3" value="${item.quantity}" /></td>`);
                                        newRow.append(`<td><input type="number" name="UnitPrice"  oninput="calculateTotal(1)" onkeypress="return isNumeric(evt);" size="5"  value="${item.unitPrice}" /></td>`);
                                        newRow.append(`<td><input type="text" name="TotalAmount" value="${item.totalAmount}" /></td>`);
                                        newRow.append(`<td><input type="text" name="Currency" value="${item.currency}" /></td>`);
                                        newRow.append(`<td>
                                                <select name="Status">
                                                  <option value="NonReturnable" ${item.status === "NonReturnable" ? "selected" : ""}>Non Returnable</option>
                                                  <option value="Returned" ${item.status === "Returned" ? "selected" : ""}>Returned</option>
                                                  <option value="Pending" ${item.status === "Pending" ? "selected" : ""}>Pending</option>
                                                </select>
                                              </td>`);
                                        newRow.append('<td><button type="button" id="btnDelete" disable>Delete</button></td>');

                                        table.append(newRow);
                                    });
                                }




                            </script>
